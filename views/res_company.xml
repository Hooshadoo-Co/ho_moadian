<odoo>
	<data>
		<record id="view_company_form_inherit_moadian" model="ir.ui.view">
			<field name="name">res.company.form.default</field>
			<field name="model">res.company</field>
			<field name="inherit_id" ref="base.view_company_form"/>
			<field name="arch" type="xml">
				<xpath expr="//page" position="after">
					<page string="Moadian Information">
						<group>
							<field name="org_api_url"/>
							<field name="fiscal_id"/>
						    <field name="pkey_file"/>
						</group>
					</page>
				</xpath>
			</field>
		</record>
	</data>
</odoo>